#ifndef GPS_PRIV_H_
#define GPS_PRIV_H_

typedef struct
{
	u8   GPGGA_LINE_FLAG:      1 ;
	u8   LAT_COMMA_FLAG :      1 ;
	u8   LONG_COMMA_FLAG:      1 ;
	u8   FIRST_DIR_FLAG :      1 ;
	u8   SECOND_DIR_FLAG:      1 ;
	u8   DOT_FLAG       :      1 ;

}GPS_FLAGS_t;





#endif /* GPS_PRIV_H_ */
